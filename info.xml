<?xml version="1.0"?>
<extension key="cpsms" type="module">
  <file>cpsms</file>
  <name>cpsms</name>
  <description>SMS provider for cpsms.dk</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Niels Erik Andersen</author>
    <email>nea@tingstroem.dk</email>
  </maintainer>
  <urls>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2023-09-05</releaseDate>
  <version>1.0</version>
  <develStage>beta</develStage>
  <compatibility>
    <ver>5.59</ver>
  </compatibility>
  <comments>This module supports outgoing single and mass SMS</comments>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Cpsms</namespace>
    <format>23.02.1</format>
    <angularModule>crmCpsms</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
  </mixins>
</extension>
